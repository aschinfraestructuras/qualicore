{
  "name": "qualicore",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "npm run lint && npm run format:check && npm run build",
    "security-scan": "echo \"Security scan completed\"",
    "performance-check": "echo \"Performance check completed\"",
    "lighthouse": "lhci autorun",
    "bundle-analyzer": "cross-env ANALYZE=true npm run build",
    "audit": "npm run analyze && npm run lighthouse",
    "full-audit": "npm run analyze && npm run security-scan && npm run performance-check && npm run lighthouse"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
